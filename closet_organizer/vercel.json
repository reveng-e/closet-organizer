{
    "version": 2,
    "builds": [
        {
            "src": "closet_organizer/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb", "runtime": "python3.13"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "closet_organizer/wsgi.py"
        }
    ]
}